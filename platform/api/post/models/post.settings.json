{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "owner": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "name": {
      "type": "string"
    },
    "details": {
      "type": "richtext"
    },
    "campaign": {
      "model": "campaign"
    }
  }
}
